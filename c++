#include <iostream>

int main() {
    std::cout << "Hello, World!" << std::endl;
    std::cout << "This is a basic C++ program." << std::endl;
    int a = 10, b = 20;
    int sum = a + b;
    std::cout << "Sum of " << a << " and " << b << " is: " << sum << std::endl;
    std::cout << "C++ is powerful!" << std::endl;
    std::cout << "Goodbye!" << std::endl;
    return 0;
}
