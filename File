def fibonacci(n):
    a, b = 0, 1
    for _ in range(n):
        print(a, end=" ")
        a, b = b, a + b

# Print first 10 Fibonacci numbers
fibonacci(10)

# feature202 changes


def reverse_string(s):
    return s[::-1]

# Example usage
text = "Hello, World!"
reversed_text = reverse_string(text)
print("Original:", text)
print("Reversed:", reversed_text)
 

#feature202 ends

 # feature201 changes
def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return False
    return True

# Check prime numbers from 1 to 20
for num in range(1, 21):
    if is_prime(num):
        print(num, "is a prime number")
#feature201 ends

