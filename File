def fibonacci(n):
    a, b = 0, 1
    for _ in range(n):
        print(a, end=" ")
        a, b = b, a + b

# Print first 10 Fibonacci numbers
fibonacci(10)
 # feature201 changes
def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return False
    return True

# Check prime numbers from 1 to 20
for num in range(1, 21):
    if is_prime(num):
        print(num, "is a prime number")
#feature201 ends