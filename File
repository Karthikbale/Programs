def fibonacci(n):
    a, b = 0, 1
    for _ in range(n):
        print(a, end=" ")
        a, b = b, a + b

# Print first 10 Fibonacci numbers
fibonacci(10)
# feature202 changes


def reverse_string(s):
    return s[::-1]

# Example usage
text = "Hello, World!"
reversed_text = reverse_string(text)
print("Original:", text)
print("Reversed:", reversed_text)
 

